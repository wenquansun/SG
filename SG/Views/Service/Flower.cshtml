@{
    ViewBag.Title = "鲜花车队";
}
@section head{

    <!-- Add mousewheel plugin (this is optional) -->
    <script type="text/javascript" src="/js/jquery.mousewheel-3.0.6.pack.js"></script>

    <!-- Add fancyBox main JS and CSS files -->
    <script type="text/javascript" src="/source/jquery.fancybox.js?v=2.1.4"></script>
    <link rel="stylesheet" type="text/css" href="/source/jquery.fancybox.css?v=2.1.4" media="screen" />


    <!-- Add Thumbnail helper (this is optional) -->
    <link rel="stylesheet" type="text/css" href="/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7" />
    <script type="text/javascript" src="/source/helpers/jquery.fancybox-thumbs.js?v=1.0.7"></script>
    <script type="text/javascript">
        jQuery(document).ready(function () {
            $('.fancybox-thumbs').fancybox({
                prevEffect: 'none',
                nextEffect: 'none',
                closeBtn: true,
                arrows: true,
                nextClick: false,
                helpers: {
                    thumbs: {
                        width: 50,
                        height: 50
                    },
                    title: {
                        type: 'inside'
                    },
                    overlay: {
                        css: {
                            'background': 'rgba(238,238,238,0.85)'
                        }
                    }
                },
                wrapCSS: 'fancybox-custom',
                closeClick: true,
                openEffect: 'none',

            });
        });
    </script>
}

<!--page_container-->
<div class="page_container">
    <div class="breadcrumb">
        <div class="wrap">
            <div class="container">
                <span>婚礼服务/</span>鲜花车队
            </div>
        </div>
    </div>
    <!--MAIN CONTENT AREA-->
    <div class="wrap">
        <div class="container inner_content">
            <section>
                <h2 class="title"><span>Tips</span></h2>
                <p style="text-indent:2em;">婚礼花艺设计的每一个环节都是具有独特品位，渗透着奢华的气质，用专业的设计师和训练有素的手工技艺叠加，呈现出震撼华丽的视觉盛宴。婚礼花艺设计从各个艺术领域、设计风格中汲取灵感，成就高级定制婚礼的高端典雅。 一辆装饰着鲜花和彩带的漂亮婚车是结婚过程中必不可少的，而气派、空间大的豪华婚车阵容则更能一圆梦想。 各种婚车供您选购 劳斯莱斯、法拉利、奔驰E系、宝马5系、奥迪A6L、奔驰敞篷、雅阁、现代等</p>
            </section>
            <br />
            <div class="row">
                <!-- portfolio_block -->
                <div class="projects">
                    <div class="span3 element category01" data-category="category01">
                        <div class="hover_img">
                            <a class="fancybox-thumbs" style="" data-fancybox-group="thumb" href="/img/xhcd/xhcd1_b.jpg" title="">
                                        <img src="/img/xhcd/xhcd1_b.jpg" alt="" /></a>
                        </div>

                    </div>
                    <div class="span3 element category02" data-category="category02">
                        <div class="hover_img">
                            <a class="fancybox-thumbs" style="" data-fancybox-group="thumb" href="/img/xhcd/xhcd2_b.jpg" title="">
                                        <img src="/img/xhcd/xhcd2_b.jpg" alt="" /></a>
                        </div>
                    </div>
                    <div class="span3 element category03" data-category="category03">
                        <div class="hover_img">
                            <a class="fancybox-thumbs" style="" data-fancybox-group="thumb" href="/img/xhcd/xhcd3_b.jpg" title="">
                                        <img src="/img/xhcd/xhcd3_b.jpg" alt="" /></a>
                        </div>
                    </div>
                    <div class="span3 element category01" data-category="category01">
                        <div class="hover_img">
                            <a class="fancybox-thumbs" style="" data-fancybox-group="thumb" href="/img/xhcd/xhcd4_b.jpg" title="">
                                        <img src="/img/xhcd/xhcd4_b.jpg" alt="" /></a>
                            <span class="shadowimg220"></span>
                        </div>
                    </div>
                    <div class="span3 element category01" data-category="category01">
                        <div class="hover_img">
                            <a class="fancybox-thumbs" style="" data-fancybox-group="thumb" href="/img/xhcd/xhcd5_b.jpg" title="">
